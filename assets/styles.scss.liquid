}

/* quick hacks for Facebook button */
.fb_iframe_widget > span { vertical-align: baseline!important; }

.fb_iframe_widget iframe {
  max-width:none;
}

.slide {
  position: relative;
}

.slide img { width: 100%; }

/* Overlays over slider and ad on homepage */
.overlay-text {
  position: absolute;
  top: 0;
  left: 0%;
  width: 100%;
  height: 100%;
  text-align: center;
  display: flex;
  align-items: center;

  // rte styles in overlays - a bit unique
  p {
    margin: 0.1em 0;
  }
}

.overlay-text.overlay-left {
  text-align: left;
}

.overlay-text.overlay-right {
  text-align: right;
}

@media (max-width: 767px) {
  .slideshow {
    .overlay-text.overlay-left,
    .overlay-text.overlay-right {
      text-align: center;
    }
  }
}

.overlay-text .inner {
  display: block;
  width: 100%;
}

.overlay-text .text > div {
  line-height: 1.2em;
  padding: 15px 1em;
  display: inline-block;
}

.slide .overlay-text .text {
  @extend h1;
  line-height: 1.4;
  margin: 0;
  padding: 0;

  .second-line {
    font-size: 60%;
    line-height: 1.2;
  }

  @media(max-width: 1160px) {
    .snap-text {
      font-size: $home-overlay-title-mobile-size;

      .second-line {
        font-size: $home-overlay-text-mobile-size;
      }
    }
  }
}

.slide .overlay-text .button:not(:first-child) {
  margin-top: 10px;
}

/* Full width ad only */
.full-width-image,
.image-block {
  overflow: hidden;
  position: relative;
  line-height: 0;

  .img-cont {
    display: block;
    position: relative;
    background-size: cover;
    background-position: center center;
    background-repeat: no-repeat;
  }
  a {
    display: block;
  }
  img {
    width: 100%;
    height: auto;
    vertical-align: top;
  }
  &.no-stretch img {
    width: auto;
  }
}

.full-width-image,
.slideshow {
  .overlay-text .text {
    font-size: $larger-text-size * {{ settings.font_size_slideshow }};
    line-height: 1.2em;
  }
}

/* purely for keeping placeholder size */
.full-width-image .rimage-outer-wrapper {
  visibility: hidden;
  max-width: none!important;
  max-height: none!important;
}

/* Onboarding placeholder */
.placeholder-image {
  position: relative;
  padding-top: 100%;
  height: 0px;
  overflow: hidden;
  background: #f9fafa;

  &.dark {
    background: #c6c7c8;
    svg, svg g {
      fill: #a3a6a9;
    }
  }
  &.landscape {
    padding-top: 55%;
  }
  &.placeholder--banner {
    padding-top: 475px;
    @media (max-width: 767px) {
      padding-top: 55%;
    }
  }
  svg {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    &, & g {
      fill: #c8cbcd;
    }
  }
}

.align-center,
.align-centre {
  text-align: center;
}

.align-right {
  text-align: right;
}

a .state {
  display: inline-block;
  width: 1em;
  text-align: center;
}

.video-container,
.product-media--frame {
  position: relative;
  padding-top: 56.25%;
  height: 0;
  overflow: hidden;
  max-width: 100%;

  iframe,
  object,
  embed,
  video,
  model-viewer,
  .shopify-model-viewer-ui,
  .media-item,
  .plyr {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
  }

  .plyr__video-wrapper {
    height: 100%;
  }
}

.video-container__cover {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 1;
  background: #eee;

  .rimage-outer-wrapper {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-size: cover;
    background-position: center center;
  }
}

.video-container__cover--playing {
  opacity: 0!important;
  pointer-events: none;
}

$_video-play-button-size: 80px;
.video-container__play {
  position: absolute;
  top: 50%;
  left: 50%;
  margin: (-$_video-play-button-size/2) 0 0 (-$_video-play-button-size/2);
  transition: transform 150ms;

  svg {
    width: $_video-play-button-size;
    height: $_video-play-button-size;
    fill: transparent;
    stroke: #fff;

    polygon {
      fill: #fff;
    }
  }

  &:hover {
    transform: scale(1.1);

    svg {
      fill: transparent;
    }
  }
}

.product-media--video[data-video-type="external"] {
  &::before,
  &::after {
    content: '';
    position: absolute;
    top: 50px;
    bottom: 70px;
    width: calc(50% - 50px);
    z-index: 1;
  }

  &::before {
    left: 0;
  }

  &::after {
    right: 0;
  }
}

.plyr.plyr--full-ui.plyr--video {
  color: $body-text-col;
  background-color: transparent;

  .plyr__poster,
  .plyr__video-wrapper {
    background-color: transparent;
  }

  // --------------------------------------------------------------
  // Fullscreen
  // --------------------------------------------------------------

  &:fullscreen .plyr__video-wrapper &:fullscreen .plyr__poster {
    background-color: #000;
  }

  /* stylelint-disable-next-line */
  &:-webkit-full-screen .plyr__video-wrapper,
  &:-webkit-full-screen .plyr__poster {
    background-color: #000;
  }

  /* stylelint-disable-next-line */
  &:-moz-full-screen .plyr__video-wrapper,
  &:-moz-full-screen .plyr__poster {
    background-color: #000;
  }

  /* stylelint-disable-next-line */
  &:-ms-fullscreen .plyr__video-wrapper,
  &:-ms-fullscreen .plyr__poster {
    background-color: #000;
  }

  // Fallback for unsupported browsers
  .plyr--fullscreen-fallback .plyr__video-wrapper,
  .plyr--fullscreen-fallback .plyr__poster {
    background-color: #000;
  }

  .plyr__control.plyr__control--overlaid {
    background-color: $body-bg-col;
    border-color: rgba($body-text-col, 0.05);

    &.plyr__tab-focus,
    &:hover {
      color: rgba($body-text-col, 0.55);
    }
  }

  .plyr__controls {
    background-color: $body-bg-col;
    border-color: rgba($body-text-col, 0.05);
  }

  .plyr__progress {
    input[type='range'] {
      &::-moz-range-thumb {
        box-shadow: 2px 0 0 0 $body-bg-col;
      }

      &::-ms-thumb {
        box-shadow: 2px 0 0 0 $body-bg-col;
      }

      &::-webkit-slider-thumb {
        box-shadow: 2px 0 0 0 $body-bg-col;
      }

      &::-webkit-slider-runnable-track {
        background-image: linear-gradient(
          to right,
          currentColor var(--value, 0),
          rgba($body-text-col, 0.6) var(--value, 0)
        );
      }

      &::-moz-range-track {
        background-color: rgba($body-text-col, 0.6);
      }

      &::-ms-fill-upper {
        background-color: rgba($body-text-col, 0.6);
      }

      &.plyr__tab-focus {
        &::-webkit-slider-runnable-track {
          box-shadow: 0 0 0 4px rgba($body-text-col, 0.25);
        }

        &::-moz-range-track {
          box-shadow: 0 0 0 4px rgba($body-text-col, 0.25);
        }

        &::-ms-track {
          box-shadow: 0 0 0 4px rgba($body-text-col, 0.25);
        }
      }

      &:active {
        &::-moz-range-thumb {
          box-shadow: 0 0 0 3px rgba($body-text-col, 0.25);
        }

        &::-ms-thumb {
          box-shadow: 0 0 0 3px rgba($body-text-col, 0.25);
        }

        &::-webkit-slider-thumb {
          box-shadow: 0 0 0 3px rgba($body-text-col, 0.25);
        }
      }
    }

    .plyr__tooltip {
      background-color: $body-text-col;
      color: $body-bg-col;

      &::before {
        border-top-color: $body-text-col;
      }
    }
  }

  &.plyr--loading .plyr__progress__buffer {
    background-image: linear-gradient(
      -45deg,
      rgba($body-text-col, 0.6) 25%,
      transparent 25%,
      transparent 50%,
      rgba($body-text-col, 0.6) 50%,
      rgba($body-text-col, 0.6) 75%,
      transparent 75%,
      transparent
    );
  }

  .plyr__volume {
    input[type='range'] {
      color: $body-bg-col;

      &::-moz-range-thumb {
        box-shadow: 2px 0 0 0 $body-text-col;
      }

      &::-ms-thumb {
        box-shadow: 2px 0 0 0 $body-text-col;
      }

      &::-webkit-slider-thumb {
        box-shadow: 2px 0 0 0 $body-text-col;
      }

      &::-webkit-slider-runnable-track {
        background-image: linear-gradient(
          to right,
          currentColor var(--value, 0),
          rgba($body-bg-col, 0.6) var(--value, 0)
        );
      }

      &::-moz-range-track,
      &::-ms-fill-upper {
        background-color: rgba($body-bg-col, 0.6);
      }

      &.plyr__tab-focus {
        &::-webkit-slider-runnable-track {
          box-shadow: 0 0 0 4px rgba($body-bg-col, 0.25);
        }

        &::-moz-range-track {
          box-shadow: 0 0 0 4px rgba($body-bg-col, 0.25);
        }

        &::-ms-track {
          box-shadow: 0 0 0 4px rgba($body-bg-col, 0.25);
        }
      }

      &:active {
        &::-moz-range-thumb {
          box-shadow: 0 0 0 3px rgba($body-bg-col, 0.25);
        }

        &::-ms-thumb {
          box-shadow: 0 0 0 3px rgba($body-bg-col, 0.25);
        }

        &::-webkit-slider-thumb {
          box-shadow: 0 0 0 3px rgba($body-bg-col, 0.25);
        }
      }
    }
  }
}

.slick-slider {
  .plyr.plyr__tab-focus,
  model-viewer.focus-visible {
    outline: none;

    &::after {
      content: '';
      pointer-events: none;
      position: absolute;
      top: 0;
      left: 0;
      height: 100%;
      width: 100%;
      box-shadow: 0 0 1px 1px $body-text-col inset;
    }
  }
}

/* hack for some model-viewer 'quirks' */
model-viewer {
  text-align: left;

  .container {
    max-width: none!important;
    margin-left: 0!important;
    margin-right: 0!important;
    padding-left: 0!important;
    padding-right: 0!important;
  }
}

.shopify-model-viewer-ui {
  button[hidden] {
    display: none;
  }

  .shopify-model-viewer-ui__controls-area {
