/* Standard and Footer disclosure for multi language and multi currency */

.footer-localization {
  text-align: center;
  margin-top: 20px;
  margin-bottom: -10px;
  padding: 0 20px;
}

.selectors-form__item {
  display: inline-block;
  margin: 10px 5px;
}

.disclosure {
  position: relative;
  font-size: $base-text-size;
  line-height: 1.2em;
}

.disclosure__toggle {
  padding: 0.75em 3em 0.75em 1em;
  border: 1px solid $input-border-col;
  background-color: $input-background-col;
  color: $input-text-col;
  white-space: nowrap;
  line-height: 1.2em;

  &:hover {
    border: 1px solid $input-border-col;
    background-color: $input-background-col;
    color: $input-text-col;
  }

  /* ul little triangle*/
  &[aria-expanded="true"] {
    &::before,
    &::after {
      content: '';
      position: absolute;
      left: 18px;
      top: 0;
      margin-top: -8px;
      width: 0;
      height: 0;
      border-style: solid;
      border-width: 8px 8px 0 8px;
      animation: disclosure-reveal-up 150ms ease-out;
    }
    &::before {
      border-color: $input-border-col transparent transparent transparent;
    }
    &::after {
      margin-top: -9px;
      border-color: $input-background-col transparent transparent transparent;
    }
  }
}

.disclosure__toggle-arrow {
  position: absolute;
  right: 10px;
  top: 10px;
  pointer-events: none;

  svg {
    height: 18px;
    width: 18px;
  }
}

.disclosure-list {
  background-color: $input-background-col;
  border: 1px solid $input-border-col;
  bottom: calc(100% + 8px);
  position: absolute;
  display: none;
  min-height: 40px;
  max-height: 300px;
  overflow-y: auto;
  margin: 0;
}

.disclosure-list--visible {
  display: block;
  animation: disclosure-reveal-up 150ms ease-out;
  position: absolute;
}

@keyframes disclosure-reveal-down {
  0%   {
    opacity: 0;
    transform: translate3d(0, -5px, 0);
  }
  100% {
    opacity: 1;
    transform: translate3d(0, 0, 0);
  }
}

@keyframes disclosure-reveal-up {
  0%   {
    opacity: 0;
    transform: translate3d(0, 5px, 0);
  }
  100% {
    opacity: 1;
    transform: translate3d(0, 0, 0);
  }
}

.disclosure-list__item {
  white-space: nowrap;
  text-align: left;
  list-style: none;

  &:first-child .disclosure-list__option {
    padding-top: 12px;
  }

  &:last-child .disclosure-list__option {
    padding-bottom: 12px;
  }
}

.disclosure-list__option {
  display: block;
  padding: 9px 40px 9px 15px;
  line-height: 1.4em;

  &,
  #pagefooter & {
    color: $body-text-col;

    &:focus,
    &:hover {
      opacity: 1;
      color: $body-text-col;
      background-color: transparentize($body-text-col, 0.94);
    }
  }
}

.disclosure-list__item--current .disclosure-list__option {
  background-color: transparentize($body-text-col, 0.94);
}



