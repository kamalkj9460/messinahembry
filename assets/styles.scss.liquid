      position: absolute;
      z-index: 10;
      top: 100%;
      left: 0;
      width: 100%;
      border-bottom: 1px solid {{ settings.head_nav_div_col }};
      opacity: 0;
      transition: opacity 250ms ease-in-out;

      &.tier-appeared {
        opacity: 1;
      }
    }
  }

  .nav-row .nav-columns {
    display: flex;
    flex-wrap: wrap;
    margin: 0 auto;
    max-width: $page-container-width;
    padding: 20px 10px 0;

    &.nav-columns--count-1,
    &.nav-columns--count-2,
    &.nav-columns--count-3,
    &.nav-columns--count-4 {
      justify-content: center;
    }

    &.nav-columns--count-1 > li,
    &.nav-columns--count-2 > li,
    &.nav-columns--count-3 > li,
    &.nav-columns--count-4 > li { width: calc(100% / 5 - 20px); }
    &.nav-columns--count-7 > li,
    &.nav-columns--count-8 > li { width: calc(100% / 4 - 20px); }
    &.nav-columns--count-6 > li { width: calc(100% / 6 - 20px); }

    > li {
      margin: 0 10px;
      width: calc(100% / 5 - 20px);
      text-align: left;

      a {
        padding-left: 0;
        padding-right: 0;
      }

      > a {
        padding-top: 0;
      }

      > ul {
        display: block;
        padding-bottom: 20px;
        text-align: left;

        > li {
          display: block;

          > a {
            display: block;
            padding-top: 4px;
            padding-bottom: 4px;
            white-space: normal;
          }
        }
      }
    }
  }

