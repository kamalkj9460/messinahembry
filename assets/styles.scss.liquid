//Overrides standard disclosure styling
.header-disclosures {
  .selectors-form__item {
    margin: 0 0 0 10px;
  }

  .disclosure {
    font-size: inherit;
    line-height: inherit;
  }

  .disclosure__toggle {
    background: transparent;
    border: none;
    color: black;
    font-size: inherit;
    line-height: inherit;
    margin: 0;
    padding: 0 22px 0 0;
    vertical-align: middle;
    font-weight:700 !important;

    &:hover {
      background: transparent;
      border: none;
      color: inherit;
    }

    &[aria-expanded="true"] {
      /* ul little triangle */
      &::before,
      &::after {
        top: auto;
        bottom: -8px;
        margin-top: auto;
        border-width: 0px 8px 8px 8px;
        animation: disclosure-reveal-down 150ms ease-out;
        z-index: 1;
      }
      &::before {
        border-color: transparent transparent $input-border-col transparent;
      }
      &::after {
        margin-bottom: -1px;
        border-color: transparent transparent $input-background-col transparent;
      }
    }
  }

  .disclosure__toggle-arrow {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    right: 3px;
    pointer-events: none;

    svg {
      height: 14px;
      width: 14px;
    }
  }

  .disclosure-list {
    top: calc(100% + 8px);
    bottom: auto;
  }

  .disclosure-list--visible {
    animation: disclosure-reveal-down 150ms ease-out;
    overflow: auto;
  }
}

