/* Theme styles */

body {
  font-size: $base-text-size;
  line-height: $base-line-height;
  color: $body-text-col;

  background: $body-bg-col;

  {% if settings.bg_image != blank %}
  background-image: url('{{ settings.bg_image | img_url: '2048x' }}');
  {% endif %}

  {% if settings.bg_method contains 'repeatxy' %}
  background-repeat:repeat;
  {% elsif settings.bg_method contains 'repeatx' %}
  background-repeat:repeat-x;
  {% elsif settings.bg_method contains 'repeaty' %}
  background-repeat:repeat-y;
  {% elsif settings.bg_method contains 'norepeat' %}
  background-repeat:no-repeat;
  {% endif %}
  {% if settings.bg_method contains 'centertop' %}
  background-position:center top;
  {% endif %}

  overflow-y: scroll;
  /* Fonts on Macs look bad without these */
  -moz-font-feature-settings: "liga";
  -moz-osx-font-smoothing: grayscale;
  -webkit-font-feature-settings: 'liga';
  -webkit-font-smoothing: antialiased;
}

$selection-color-text: {{ settings.bg_col }};
$selection-color-bg: {{ settings.font_col_heads }};
@if alpha($selection-color-text) > 0 {
  ::selection{/* Webkit & Opera */
    background: $selection-color-bg;
    color: $selection-color-text;
  }
  ::-moz-selection{ /* Firefox */
    background: $selection-color-bg;
    color: $selection-color-text;
  }
}

a,
a:visited {
  color:{{ settings.font_col_link }};
  -moz-transition: color 100ms, border-color 100ms;
  -webkit-transition: color 100ms, border-color 100ms;
  transition: color 100ms, border-color 100ms;
  text-decoration:none;
}

a:hover {
  color:{{ settings.font_col_link_hover }};
}

select {
  max-width: 100%;
  font-family: inherit;
  font-weight: inherit;
  font-style: inherit;
}

$input-background-col: {{ settings.bg_col }};
$input-text-col: {{ settings.font_col_base }};
$input-border-col: mix($input-text-col, $input-background-col, 15%);
$input-border-col-active: {{ settings.font_col_base }};

input,
textarea {
  font-size: {{ settings.font_size_base_int }}px;
  line-height: {{ settings.font_size_base_int }}px;
  font-family: {{ base_font.family }}, {{ base_font.fallback_families }};
  font-weight: {{ base_font.weight }};
  font-style: {{ base_font.style }};
  background: $input-background-col;
  color: $input-text-col;
  border: 1px solid $input-border-col;
  padding: $input-btn-padding-vert $input-padding-hori;
  margin: 0;
  vertical-align: middle;
  max-width: 100%;
  border-radius: 0;
  -webkit-appearance: none; // ios input shadow
  box-sizing: border-box;

  &:focus {
    border-color: $input-border-col-active;
  }
}

input[type=checkbox] {
  border:none;
  -moz-border-radius:0;
  -webkit-border-radius:0;
  border-radius:0;
  -webkit-appearance: checkbox;
}

textarea {
  height:120px;
  padding-top: 10px;
  padding-bottom: 10px;
}

/* HTML5 input placeholders */
::-webkit-input-placeholder {
  color: {{ settings.font_col_base }};
}

:-moz-placeholder { /* Firefox 18- */
  color: {{ settings.font_col_base }};
}

::-moz-placeholder {  /* Firefox 19+ */
  color: {{ settings.font_col_base }};
}

:-ms-input-placeholder {
  color: {{ settings.font_col_base }};
}

.input--large {
  padding-top: $input-btn-padding-vert-large;
  padding-bottom: $input-btn-padding-vert-large;
  height: $input-btn-padding-vert-large * 2 + {{ settings.font_size_base_int }}px + 2px;
}

.button,
a.button,
button,
input[type="submit"],
input[type="reset"],
input[type="button"] {
  background: {{ settings.btn_col }};
  border: 1px solid {{ settings.btn_col }};
  color:{{ settings.btn_col_txt }};
  font-size: $base-text-size;
  font-family: inherit;
  font-weight: inherit;
  font-style: inherit;
  line-height: 1em;
  height: auto;
  margin: 0;
  text-decoration: none !important;
  cursor: pointer;
  padding: $input-btn-padding-vert $btn-padding-hori;
  vertical-align: middle;
  text-align: center;
  box-sizing: content-box;
  border-radius:0;
  transition: background-color 100ms, color 100ms, border-color 100ms, opacity 100ms;
  display: inline-block;
  font-family: {{ base_font.family }}, {{ base_font.fallback_families }};
  font-weight: {{ base_font.weight }};
  font-style: {{ base_font.style }};

  svg {
    fill: {{ settings.btn_col_txt }};
  }

  .icon {
    position: relative;
    display: inline-block;
    vertical-align: middle;
    height: 1em;
    width: 28px;

    svg {
      position: absolute;
      top: 50%;
      left: 0;
      margin-top: -12px;
    }
  }

  &:hover {
    background: {{ settings.btn_col_alt }};
    border-color: {{ settings.btn_col_alt }};
  }

  &.button--large {
    padding: $input-btn-padding-vert-large $btn-padding-hori;
    font-size: {{ settings.font_size_base_int }}px;
  }
}

.button.compact,
button.compact,
input[type="submit"].compact,
input[type="reset"].compact,
input[type="button"].compact {
  width: auto;
  min-width: 0;
  padding: $input-btn-padding-vert + 1;
}

.button.altcolour,
button.altcolour,
input[type="submit"].altcolour,
input[type="reset"].altcolour,
input[type="button"].altcolour {
  background: {{ settings.btn_alt_bg }};
  color: {{ settings.btn_alt_txt }};

  $_body_bg: {{ settings.bg_col }};
  $_alt_btn_txt: {{ settings.btn_alt_txt }};
  @if $_body_bg == $_alt_btn_txt {
    border: 1px solid {{ settings.btn_alt_bg }};
  } @else {
    border: 1px solid {{ settings.btn_alt_txt }};
  }

  svg {
    fill: {{ settings.btn_alt_txt }};
  }

  &:hover {
    background: {{ settings.btn_alt_txt }};
    color: {{ settings.btn_alt_bg }};
    $_alt_btn_bg: {{ settings.btn_alt_bg }};
    @if $_body_bg == $_alt_btn_bg {
      border-color: {{ settings.btn_alt_txt }};
    } @else {
      border-color: {{ settings.btn_alt_bg }};
    }

    svg {
      fill: {{ settings.btn_alt_bg }};
    }
  }
}

.button.notabutton,
a.button.notabutton,
button.notabutton,
input[type="submit"].notabutton {
  background: none;
  border: none;
  padding: 0;
  margin: 0;
  font-size: inherit;
  line-height: inherit;
  font-family: inherit;
  letter-spacing: inherit;
  text-transform: none;
  vertical-align: baseline;
  min-width: 0;
  overflow: visible;
  height: auto;
  color:{{ settings.font_col_link }};
  -moz-transition: color 100ms;
  -webkit-transition: color 100ms;
  transition: color 100ms;

  &:hover {
    color:{{ settings.font_col_link_hover }};
  }
}

.button.disabled,
a.button.disabled,
button[disabled],
input[type="submit"][disabled],
input[type="reset"][disabled],
input[type="button"][disabled] {
  background: #999 !important;
  color: #fff !important;
  border-color: #999 !important;
}

.button__icon {
  display: inline-block;
  vertical-align: middle;
  position: relative;
  width: 24px;
  height: {{ settings.font_size_base_int }}px;

  svg {
    position: absolute;
    left: 0;
    top: 50%;
    margin-top: -12px;
  }
  svg.feather {
    fill: transparent;
    stroke: {{ settings.btn_col_txt }};
    stroke-width: 1;
  }
}

h1, .h1-style, h2, h3, h4, h5, h6 {
  color: {{ settings.font_col_heads }};
  line-height: 1.2em;
  margin-bottom: 10px;
  svg, svg g {
    fill: {{ settings.font_col_heads }};
  }
}

h1 a, h2 a, h3 a, h4 a, h5 a, h6 a,
h1 a:visited, h2 a:visited, h3 a:visited, h4 a:visited, h5 a:visited, h6 a:visited {
  color: inherit;
  text-decoration: none;
}

h1 a:hover, h2 a:hover, h3 a:hover, h4 a:hover, h5 a:hover, h6 a:hover {
  color:{{ settings.font_col_link_hover }};
