/* Gallery viewer */
$gallery-viewer-mobile-breakpoint: 600px;
$gallery-viewer-icon-size-desktop: 34px;
$gallery-viewer-icon-size-mobile: 26px;
$gallery-viewer-icon-colour: #000;
$gallery-viewer-icon-bg: rgba(255, 255, 255, 0.7);
$gallery-viewer-thumbnail-border-size: 4px;
$gallery-viewer-thumbnail-size-desktop: 69px;
$gallery-viewer-thumbnail-size-mobile: 50px;

.gallery-viewer {
  position: fixed;
  z-index: 10000;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: #fff;
  transition: opacity 250ms ease-out, transform 250ms ease-out;
}

.gallery-viewer--pre-reveal,
.gallery-viewer--transition-out {
  opacity: 0;
  transform: scale(0.97);
}

.gallery-viewer__zoom {
  position: absolute;
  z-index: 1;
  width: 100%;
  height: 100%;
  pointer-events: none;
}

.gallery-viewer__zoom-image {
  position:absolute;
  width: 60%;
  height: auto;
  max-width: none;
  max-height: none;
  min-width: 0;
  min-height: 0;
  left:21%;
}

.gallery-viewer__thumbs {
  position: absolute;
  left:21% !important;
  z-index: 2;
  bottom: 20px;
  width: 60%;
  text-align: center;
  overflow: auto;
  white-space: nowrap;
}

.gallery-viewer__thumb {
  display: inline-block;
  padding: $gallery-viewer-thumbnail-border-size;
  background: #fff;
  width: $gallery-viewer-thumbnail-size-desktop;

  &:not(:first-child) {
    margin-left: - $gallery-viewer-thumbnail-border-size;
  }

  @media (max-width: $gallery-viewer-mobile-breakpoint) {
    width: $gallery-viewer-thumbnail-size-mobile;
  }
}

.gallery-viewer__thumb--active {
  position: relative;

  &::after {
    content: '';
    position: absolute;
    display: block;
    bottom: $gallery-viewer-thumbnail-border-size;
    left: $gallery-viewer-thumbnail-border-size;
    right: $gallery-viewer-thumbnail-border-size;
    height: 3px;
    background: $thumbnail-highlight-col;
  }
}

.gallery-viewer__controls {
  position: relative;
  height: 100%;
  z-index: 3;
  pointer-events: none;
}

.gallery-viewer__button {
  position: absolute;
  pointer-events: auto;
  padding: 5px;
  background: $gallery-viewer-icon-bg;
  color: $gallery-viewer-icon-colour;

  svg {
    width: $gallery-viewer-icon-size-desktop;
    height: $gallery-viewer-icon-size-desktop;

    @media (max-width: $gallery-viewer-mobile-breakpoint) {
      width: $gallery-viewer-icon-size-mobile;
      height: $gallery-viewer-icon-size-mobile;
    }
  }

  svg.feather {
    stroke: $gallery-viewer-icon-colour;
  }
}

.gallery-viewer__prev,
.gallery-viewer__next {
  top: calc(50% - #{ $gallery-viewer-icon-size-desktop / 2 });

  @media (max-width: $gallery-viewer-mobile-breakpoint) {
    top: calc(50% - #{ $gallery-viewer-icon-size-mobile / 2 });
  }
}

.gallery-viewer__prev {
  left: 0;
}

.gallery-viewer__next {
  right: 0;
}

.gallery-viewer__close {
  top: 0;
  right: 0;
}

.gallery-viewer--single-image .gallery-viewer__prev,
.gallery-viewer--single-image .gallery-viewer__next,
.gallery-viewer--single-image .gallery-viewer__thumbs {
  display: none;
}

// prevent page scroll on touchscreens
@media (pointer: coarse) {
  .gallery-viewer-open,
  .gallery-viewer-open body {
    overflow: hidden;
    height: 100%;
  }
}

