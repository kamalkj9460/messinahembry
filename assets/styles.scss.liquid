/* Content and content layouts */

.spaced-row {
  margin-bottom: $section-padding;
}

.spaced-row-above {
  margin-top: $section-padding;
}

.fully-spaced-row {
  margin-top: $section-padding;
  margin-bottom: $section-padding;
}

.strongly-spaced-row {
  margin-top: $section-padding*1.5;
  margin-bottom: $section-padding*1.5;
}

.very-strongly-spaced-row {
  margin-top: $section-padding*2;
  margin-bottom: $section-padding*2;
}

.huge-spaced-row {
  margin-top: $section-padding*3;
  margin-bottom: $section-padding*3;
}

.lightly-spaced-row-above {
  margin-top: $section-padding / 2;
}

.lightly-spaced-row {
  margin-bottom: 1em;
}

.padded-row {
  padding-top: $section-padding;
  padding-bottom: $section-padding;

  &.no-bg {
    margin-top: $section-padding;
    margin-bottom: $section-padding;
    padding-top: 0;
    padding-bottom: 0;
  }
}

.strongly-padded-row {
  padding-top: $section-padding*2;
  padding-bottom: $section-padding*2;
}

.content img,
.content iframe,
.content embed,
.content object {
  max-width: 100%;
  height: auto;
}

.layout-column-half-left {
  float: left;
  width: 45%;
}

.layout-column-half-right {
  float: right;
  width: 45%;
}

.layout-content-with-image-left,
.layout-content-with-image-right {
  &.valign-top {
     .image-cont, .content {
       vertical-align: top;
     }
  }
  &.valign-middle {
    .image-cont, .content {
       vertical-align: middle;
     }
  }
  &.valign-bottom {
    .image-cont, .content {
       vertical-align: bottom;
     }
  }
}

.layout-content-with-image-left .image-cont,
.layout-content-with-image-right .image-cont {
  display: inline-block;

  width: 33.333%;
  text-align: center;
}

.layout-content-with-image-left .content,
.layout-content-with-image-right .content {
  display: inline-block;

  width: 60%;
  margin-right: 6.666%;
}

.layout-content-with-image-left .content {
  margin-left: 5%;
  margin-right: 0;
}

.layout-content-with-image--large-image {
  .image-cont {
    width: 60%;
  }
  .content {
    width: 33.333%;
  }
}

$_micro-collage-unit-width: 14.5%;
.micro-collage {
  position: relative;
  height: 0px;
  padding-top: 60%;

  @media (max-width: 767px) {
    padding-top: 100%;
  }

  .fade-in {
    opacity: 0;
    transition: opacity 500ms;
  }
  .fade-in.lazyloaded {
    opacity: 1;
  }

  .placeholder-image {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    padding: 0;
  }

  .micro-collage__image {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-size: cover;
    background-position: center center;
  }

  .micro-collage__image-1-frame {
    position: absolute;
    z-index: 2;
    transform: translate(0, 0);
    transition: all 200ms ease-out;
    outline: 1px solid transparent; // required rendering hack

    left: $_micro-collage-unit-width;
    right: 0;
    top: $_micro-collage-unit-width * 1.5;
    bottom: $_micro-collage-unit-width * 1.5;

    &.lazyload--placeholder {
      background: $loading-image-bg;
    }
  }

  .micro-collage__image-2-frame {
    position: absolute;
    z-index: 1;
    transform: translate(0, 0);
    transition: all 200ms ease-out;
    outline: 1px solid transparent; // required rendering hack

    left: 0;
    right: $_micro-collage-unit-width * 2.5;
    top: $_micro-collage-unit-width / 2;
    bottom: $_micro-collage-unit-width / 2;

    .placeholder-image {
      opacity: 0.25;
    }

    &.lazyload--placeholder {
      background: $loading-image-bg;
    }
  }

  @media (min-width: 768px) {
    &.micro-collage--link-hover {
      .micro-collage__image-1-frame {
        transform: scale(1.03);
        //transform: translate(10px, -10px);
      }
      .micro-collage__image-2-frame {
        transform: scale(0.97);
        //transform: translate(-10px, 10px);
      }
      &.micro-collage--style-2,
      &.micro-collage--style-4 {
        .micro-collage__image-1-frame {
          transform: scale(1.03);
          //transform: translate(0, -10px);
        }
        .micro-collage__image-2-frame {
          transform: scale(0.97);
          //transform: translate(0, 10px);
        }
      }
    }
  }

  &.micro-collage--style-1,
  &.micro-collage--style-4 {
    margin-top: - $_micro-collage-unit-width / 4;
    margin-bottom: - $_micro-collage-unit-width / 4;
  }

  &.micro-collage--style-3 {
    margin-top: - $_micro-collage-unit-width / 2;
    margin-bottom: - $_micro-collage-unit-width / 2;
  }

  &.micro-collage--style-2 {
    margin-top: - $_micro-collage-unit-width / 4;
    margin-bottom: - $_micro-collage-unit-width / 4;

    .micro-collage__image-1-frame {
      left: 0;
      right: $_micro-collage-unit-width;

      @media (max-width: 767px) {
        left: $_micro-collage-unit-width / 2;
        right: $_micro-collage-unit-width / 2;
      }

      .layout-content-with-image-right & {
        left: $_micro-collage-unit-width;
        right: 0;
        @media (max-width: 767px) {
