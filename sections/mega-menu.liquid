<section id="main-menu">
  <div class="d-none d-lg-block" id="main-menu-desktop">
    <div class="container">
      <nav role="navigation">
        <ul class="d-none d-lg-flex reset">
          {% for menu in section.blocks %}
            <li class="menu-parent {% if menu.settings.full_width %}menu-full{% endif %}">
              <a
                class="{% if menu.settings.enable_sale_color %}menu-sale-color{% endif %} {% if menu.settings.title_lowercase %}text-acc{% endif %}"
                href="{{ menu.settings.url }}"
              >
                <span> {{ menu.settings.title }} </span>
                {% if menu.settings.enable_dropdown != blank %}
                  <span
                    ><span><i class="fa fa-minus" style="color:black; font-size:10px;"></i></span
                    ><i class="fa fa-plus" style="color:black; font-size:10px;"></i
                  ></span>
                {% endif %}
              </a>
              {%- if menu.settings.enable_dropdown -%}
                <div class="menu-dropdown-container">
                  <div class="container">
                    <div class="row justify-content-center">
                      {%- include 'mega-menu-node', node: 'one', data: menu.settings -%}
                      {%- include 'mega-menu-node', node: 'two', data: menu.settings -%}
                      {%- include 'mega-menu-node', node: 'three', data: menu.settings -%}
                      {%- include 'mega-menu-node', node: 'four', data: menu.settings -%}
                      {%- include 'mega-menu-node', node: 'five', data: menu.settings -%}
                    </div>
                  </div>
                  {%- include 'mega-menu-view-all', data: menu.settings -%}
                </div>
              {%- endif -%}
            </li>
          {% endfor %}
        </ul>
      </nav>
    </div>
  </div>

  <div id="main-menu-mobile">
    <div class="mobile-menu-overlay" data-toggle="offcanvas"></div>
    <div class="mobile-menu">
      <nav role="navigation">
        {%- assign mobile_menu_list = section.settings.mobile_menu_list -%}
        {% if linklists[mobile_menu_list].links.size > 0 %}
          <ul>
            {% for mobile_parent in linklists[mobile_menu_list].links %}
              <li class="menu-parent {% if mobile_parent.links.size > 0 %}menu-has-children{% endif %}">
                <div class="level-1">
                  {%- if mobile_parent.links.size > 0 -%}
                    <span data-toggle="mobile-menu-node">
                      <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24">
                        <path d="M8.59 16.34l4.58-4.59-4.58-4.59L10 5.75l6 6-6 6z"/>
                      </svg>
                      <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24">
                        <path d="M7.41 7.84L12 12.42l4.59-4.58L18 9.25l-6 6-6-6z"/>
                      </svg>
                    </span>
                  {%- endif -%}
                  <a href="{{ mobile_parent.url }}">
                    <span>
                      {{ mobile_parent.title }}
                    </span>
                  </a>
                </div>

                {%- if mobile_parent.links.size > 0 -%}
                  <div class="menu-dropdown-container">
                    {% for mobile_child in mobile_parent.links %}
                      <div class="col-lg-12">
                        <div class="menu-node">
                          <div class="node-heading node-heading-linked">
                            <a href="{{ mobile_child.url }}">{{ mobile_child.title }}</a>
                          </div>
                        </div>
                      </div>
                    {% endfor %}
                  </div>
                {%- endif -%}
              </li>
            {% endfor %}
          </ul>
        {% endif %}
      </nav>
    </div>
  </div>
</section>

